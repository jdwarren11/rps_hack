<div class='large-2 columns'>
    <table>
      <tr>
        <th>Player 1</th>
        <th>Player 2</th>
      </tr>
   <% @games.each do |game| %>
      <tr>
        <td><%= game['p1_move'] %></td>
        <td><%= game['p2_move'] %></td>
      </tr>
    <% end %>
  </table>
</div>

<% if !@current_match.winner %>
  <div class="large-10 columns text-center game_moves">
    <a href= " /gameplay/move/<%='rock'%>/user_id/<%=session[:user_id]%>/match_id/<%=params[:match_id]%>" > <img src="../img/rock.png" /></a>
    <a href=" /gameplay/move/<%='paper'%>/user_id/<%=session[:user_id]%>/match_id/<%=params[:match_id]%>"><img src="../img/paper.png" /></a>
    <a href=" /gameplay/move/<%='scissors'%>/user_id/<%=session[:user_id]%>/match_id/<%=params[:match_id]%>"><img src="../img/scissors.png"/></a>
  </div>
<% else %>
  <h3>GAME OVER <%= @current_match.winner %></h3>
<% end %>